{
  "buildCommand": "npx vite build",
  "outputDirectory": "dist/public",
  "framework": "vite",
  "installCommand": "npm install",
  "rewrites": [
    {
      "source": "/api/:path*",
      "destination": "https://price-production-c1cb.up.railway.app/api/:path*"
    },
    {
      "source": "/logout",
      "destination": "https://price-production-c1cb.up.railway.app/logout"
    },
    {
      "source": "/((?!api|logout).*)",
      "destination": "/index.html"
    }
  ]
}
